<!--- Set up the application. --->	
<cfset THIS.Name = "@@Name" />


<cfset THIS.sessionmanagement = @@sessionmanagement  />

<cfset THIS.sessiontimeout = @@sessiontimeout /><!--- usually a createTimeSpan() --->

<cfset THIS.applicationtimeout = @@applicationtimeout /><!--- usually a createTimeSpan() --->

<cfset THIS.clientmanagement = @@clientmanagement />

<cfset THIS.clientstorage = "@@clientstorage" />

<cfset THIS.loginstorage = "@@loginstorage" />

<cfset THIS.scriptprotect = "@@scriptprotect" />

<cfset THIS.setclientcookies = @@setclientcookies />

<cfset THIS.setdomaincookies = @@setdomaincookies />

<cfset THIS.mappings = structNew() />


<!--- FARCRY SPECIFIC --->
<cfset THIS.locales = "@@locales" />

<cfset THIS.dsn = "@@dsn" /> 

<cfset THIS.dbType = "@@dbType" /> 

<cfset THIS.dbOwner = "@@dbOwner" /> 

<cfset THIS.plugins = "@@plugins" /> 

<!--- 
THE VIRTUAL WEBSERVER PROJECT FOLDER
 --->
<cfset THIS.projectURL = "@@projectURL" /><!--- Defaults to application name --->
<cfset THIS.webtopURL = "@@webtopURL" />

<!--- 
A key that can be used on updateapp url variable to update the application scope
Only administrators can updateapp=1
 --->
<cfset THIS.updateappKey = "1" />

<!--- 
THE NAME OF THE FOLDER THAT CONTAINS YOUR FARCRY PROJECT
SET THIS VALUE IF IT IS DIFFERENT FROM THE APPLICATION NAME
 --->
<cfset THIS.projectDirectoryName = "@@projectDirectoryName" /><!--- Defaults to application name --->

<!--- Define the page request properties. --->

<!--- <cfsetting requesttimeout="30" /> --->
<!--- <cfsetting showdebugoutput="true" /> --->
<!--- <cfsetting enablecfoutputonly="true" /> --->